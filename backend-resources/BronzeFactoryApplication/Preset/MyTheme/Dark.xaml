<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:hc="https://handyorg.github.io/handycontrol">
    <ResourceDictionary.MergedDictionaries>
        <hc:ColorPaletteResources
            TargetTheme="Dark"
            RegionColor="#232936"
            SecondaryRegionColor="#22252c"
            BackgroundColor="#22252c"
            TitleColor="#232936"
            PrimaryTextColor="#f8f8ff"
            PrimaryColor="#326cf3" 
            LightPrimaryBrush="#242e44"
            DarkPrimaryBrush="#273a67"
            BorderColor="#263540">
            <SolidColorBrush x:Key="DimmedTextBrush" Color="#d6e2fd"/>
            <!--previous secondary Brush #eee8a9 , current one has a little more contrast-->
            <SolidColorBrush x:Key="SecondaryBrush"
                             Color="#F5E58C"/>
                <SolidColorBrush x:Key="TertiaryBrush" Color="#8c8db9"/>
        </hc:ColorPaletteResources>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>
