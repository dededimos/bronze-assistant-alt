<!--The Current theme Dictionary (Values are Updated Here at Runtime through the Settings Configuration)-->
<!--Colors here are the Default Dark Colors - Though they can be Changed to Any Color to Inspect - The True Theme is set at Runtime-->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="ThemeDictionaryName">CurrentThemeDictionary</sys:String>
    <SolidColorBrush x:Key="CDarkBlackLightWhite" Color="Black"/>
    <SolidColorBrush x:Key="CTextBrushPrimary" Color="White"/>
    <SolidColorBrush x:Key="CBackgroundColor" Color="#1c1c1c"/>
    <SolidColorBrush x:Key="CBackgroundSecondaryColor" Color="#2d2d30"/>
    <SolidColorBrush x:Key="CBackgroundTertiaryColor" Color="#424242"/>
    <SolidColorBrush x:Key="CAccentColor" Color="#044289"/>
    <SolidColorBrush x:Key="CAccent2Color" Color="#BFA674"/>
    <SolidColorBrush x:Key="CHeaderBackground" Color="#044289"/>
    <SolidColorBrush x:Key="CHeaderText" Color="White"/>
    <SolidColorBrush x:Key="CMaskColor" Color="#40000000"/>
</ResourceDictionary>