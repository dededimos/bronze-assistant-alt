<UserControl x:Class="BronzeFactoryApplication.Views.ComponentsUC.CabinPropertiesUserControls.AdvancedCabinPropsUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:b="clr-namespace:BronzeFactoryApplication.Helpers.Behaviours"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:local="clr-namespace:BronzeFactoryApplication.Views.ComponentsUC.CabinPropertiesUserControls"
             xmlns:vm="clr-namespace:BronzeFactoryApplication.ViewModels.CabinsViewModels"
             xmlns:EnumSource="clr-namespace:BronzeFactoryApplication.Helpers.MarkupExtensions"
             xmlns:HbEnums="clr-namespace:ShowerEnclosuresModelsLibrary.Models.SeriesModels.HBModels;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:vmc="clr-namespace:BronzeFactoryApplication.ViewModels.CabinsViewModels.ConstraintsViewModels"
             xmlns:cc="clr-namespace:TasCustomControlsLibrary.CustomControls;assembly=TasCustomControlsLibrary"
             d:DataContext="{d:DesignInstance Type=vm:ConstraintsViewModel}"
             mc:Ignorable="d">
    <Grid x:Name="MainGrid">
        <Grid.Resources>
            <Style TargetType="{x:Type cc:Icon}"
                   BasedOn="{StaticResource CInfoTooltipIcon}">
                <Setter Property="ToolTipService.Placement"
                        Value="Right" />
                <Setter Property="ToolTipService.HorizontalOffset"
                        Value="0" />
                <Setter Property="ToolTipService.VerticalOffset"
                        Value="-17.5" />
                <Setter Property="IsTabStop"
                        Value="False" />
            </Style>
            <Style TargetType="{x:Type TextBox}"
                   BasedOn="{StaticResource TextBoxExtend}">
                <Setter Property="ToolTipService.Placement"
                        Value="Right" />
                <Setter Property="ToolTipService.VerticalOffset"
                        Value="-17.5" />
                <Setter Property="ToolTipService.InitialShowDelay"
                        Value="0" />
                <Setter Property="ToolTipService.ShowsToolTipOnKeyboardFocus"
                        Value="True" />
                <Setter Property="MinWidth"
                        Value="190" />
            </Style>
        </Grid.Resources>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition x:Name="Header1Row"
                           Height="auto" />
            <RowDefinition x:Name="ValidValuesRow"
                           Height="auto" />
            <RowDefinition x:Name="CommonConstraintsRow"
                           Height="auto" />
            <RowDefinition x:Name="MarginRow"
                           Height="20" />
            <RowDefinition x:Name="Header2Row"
                           Height="auto" />
            <RowDefinition x:Name="SpecificConstraintsRow"
                           Height="auto" />
        </Grid.RowDefinitions>

        <!--#region 1. Common Constraints Properties -->

        <StackPanel Grid.Row="0"
                    Grid.Column="0">
            <TextBlock Text="{StaticResource lngCommonConstraints}"
                       Style="{StaticResource CHeaderTextBlock}"
                       TextDecorations="Underline"
                       Margin="0,0,0,10" />
        </StackPanel>

        <StackPanel x:Name="CommonConstraintsStackPanel"
                    Orientation="Horizontal"
                    HorizontalAlignment="Center"
                    Grid.Row="1"
                    Grid.Column="0">
            <StackPanel>

                <!--#region A. MaxLength -->
                <StackPanel x:Name="MaxLengthStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngMaxPossibleLengthTooltip}" />
                        <TextBlock x:Name="MaxLengthTextBlock"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngMaxPossibleLength}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="MaxLengthTextBox"
                             d:Text="1000"
                             Text="{Binding MaxPossibleLength ,Mode=TwoWay, TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.MaxPossibleLength, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region B. MinLength -->
                <StackPanel x:Name="MinLengthStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngMinPossibleLengthTooltip}" />
                        <TextBlock x:Name="MinLengthTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngMinPossibleLength}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="MinLengthTextBox"
                             Text="{Binding MinPossibleLength ,Mode=TwoWay , TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.MinPossibleLength, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region C. MaxHeight -->
                <StackPanel x:Name="MaxHeightStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngMaxPossibleHeightTooltip}" />
                        <TextBlock x:Name="MaxHeightTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngMaxPossibleHeight}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="MaxHeightTextBox"
                             Text="{Binding MaxPossibleHeight ,Mode=TwoWay , TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.MaxPossibleHeight, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region D. MinHeight -->
                <StackPanel x:Name="MinHeightStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngMinPossibleHeightTooltip}" />
                        <TextBlock x:Name="MinHeightTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngMinPossibleHeight}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="MinHeightTextBox"
                             Text="{Binding MinPossibleHeight ,Mode=TwoWay , TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.MinPossibleHeight, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region E. MinStepHeight -->
                <StackPanel x:Name="MinStepHeightStackPanel"
                            Margin="0,5"
                            IsEnabled="{Binding CanHaveStep}">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngMinPossibleStepHeightTooltip}" />
                        <TextBlock x:Name="MinStepHeightTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngMinPossibleStepHeight}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="MinStepHeightTextBox"
                             Text="{Binding MinPossibleStepHeight ,Mode=TwoWay , TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.MinPossibleStepHeight, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

            </StackPanel>

            <hc:Divider Style="{StaticResource VerticalDivider}" />

            <StackPanel>

                <!--#region F. TolleranceMinDefaultMin -->
                <StackPanel x:Name="TolleranceMinDefaultMinStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngTolleranceMinusDefaultMinimumTooltip}" />
                        <TextBlock x:Name="TolleranceMinDefaultMinTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngTolleranceMinusDefaultMinimum}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="TolleranceMinDefaultMinTextBox"
                             Text="{Binding TolleranceMinusDefaultMinimum ,Mode=TwoWay , TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.TolleranceMinusDefaultMinimum, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region G. HeightBreakPointGlassThickness -->
                <StackPanel x:Name="HeightBreakPointThicknessStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngHeightBreakPointGlassThicknessTooltip}" />
                        <TextBlock x:Name="HeightBreakPointThicknessTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngHeightBreakPointGlassThickness}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="HeightBreakPointThicknessTextBox"
                             Text="{Binding HeightBreakPointGlassThickness ,Mode=TwoWay ,TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.HeightBreakPointGlassThickness, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region H. LengthBreakPointGlassThickness -->
                <StackPanel x:Name="LengthBreakPointThicknessStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngLengthBreakPointGlassThicknessTooltip}" />
                        <TextBlock x:Name="LengthBreakPointThicknessTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngLengthBreakPointGlassThickness}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="LengthBreakPointThicknessTextBox"
                             Text="{Binding LengthBreakPointGlassThickness ,Mode=TwoWay , TargetNullValue=''}">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.LengthBreakPointGlassThickness, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior Precision="0"
                                                           InputMode="DigitInput"
                                                           JustPositivDecimalInput="True" />
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region I. BreakPointGlassThickness -->
                <StackPanel x:Name="BreakPointThicknessStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngBreakPointMinThicknessTooltip}" />
                        <TextBlock x:Name="BreakPointThicknessTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngBreakPointMinThickness}">
                        </TextBlock>
                    </DockPanel>
                    <ComboBox x:Name="BreakPointThicknessComboBox"
                              ItemsSource="{Binding ValidThicknesses}"
                              SelectedItem="{Binding BreakPointMinThickness , Mode=TwoWay}">
                        <ComboBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.BreakPointMinThickness , Converter={StaticResource languageConverter}, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </ComboBox.ToolTip>
                        <ComboBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Converter={StaticResource languageConverter} , TargetNullValue=''}" />
                            </DataTemplate>
                        </ComboBox.ItemTemplate>
                    </ComboBox>
                </StackPanel>
                <!--#endregion-->

                <!--#region J. FinalHeightCorrection -->
                <StackPanel x:Name="FinalHeightCorrectionStackPanel"
                            Margin="0,5">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngFinalHeightCorrectionTooltip}" />
                        <TextBlock x:Name="FinalHeightCorrectionTextBlock"
                                   DockPanel.Dock="Left"
                                   Style="{StaticResource TitleTextBlockStyle}"
                                   Text="{StaticResource lngFinalHeightCorrection}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox x:Name="FinalHeightCorrectionTextBox"
                             Text="{Binding FinalHeightCorrection ,Mode=TwoWay , TargetNullValue=''}"
                             IsEnabled="{Binding CanEditFinalHeight , Mode=OneWay}"
                             d:Text="1200">
                        <TextBox.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                <TextBlock Text="{Binding Defaults.FinalHeightCorrection, StringFormat={}{0}mm}" />
                            </StackPanel>
                        </TextBox.ToolTip>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

            </StackPanel>
        </StackPanel>
        <!--#endregion-->

        <!--#region 2. Valid Values Container -->
        <Border Grid.Row="2"
                Grid.Column="0"
                Margin="0,20,0,0"
                Padding="10"
                BorderThickness="1"
                BorderBrush="{StaticResource BorderBrush}"
                CornerRadius="10"
                HorizontalAlignment="Center">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition x:Name="ColumnForMargin"
                                      Width="20" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <!--#region A. Valid Draw Numbers -->
                <DockPanel  Grid.Row="0"
                            Grid.Column="0">
                    <cc:Icon>
                        <cc:Icon.ToolTip>
                            <ListBox ItemsSource="{Binding ValidDrawNumbers}"
                                     BorderThickness="0">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter}}" />
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </cc:Icon.ToolTip>
                    </cc:Icon>
                    <TextBlock Text="{StaticResource lngValidDrawNumbers}"
                               Style="{StaticResource TitleTextBlockStyle}" />
                </DockPanel>
                <!--#endregion-->

                <!--#region B. Valid GlassFinish -->
                <DockPanel Grid.Row="0"
                           Grid.Column="2">
                    <cc:Icon>
                        <cc:Icon.ToolTip>
                            <ListBox ItemsSource="{Binding ValidGlassFinishes}"
                                     BorderThickness="0">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter}}" />
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </cc:Icon.ToolTip>
                    </cc:Icon>
                    <TextBlock Text="{StaticResource lngValidGlassFinishes}"
                               Style="{StaticResource TitleTextBlockStyle}" />
                </DockPanel>
                <!--#endregion-->

                <!--#region C. Valid Thicknesses -->
                <DockPanel Grid.Row="1"
                           Grid.Column="0">
                    <cc:Icon>
                        <cc:Icon.ToolTip>
                            <ListBox ItemsSource="{Binding ValidThicknesses}"
                                     BorderThickness="0">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter}}" />
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </cc:Icon.ToolTip>
                    </cc:Icon>
                    <TextBlock Text="{StaticResource lngValidThicknesses}"
                               Style="{StaticResource TitleTextBlockStyle}" />
                </DockPanel>
                <!--#endregion-->

                <!--#region D. Valid MetalFinish -->
                <DockPanel Grid.Row="1"
                           Grid.Column="2">
                    <cc:Icon>
                        <cc:Icon.ToolTip>
                            <ListBox ItemsSource="{Binding ValidMetalFinishes}"
                                     BorderThickness="0">
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter}}" />
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </cc:Icon.ToolTip>
                    </cc:Icon>
                    <TextBlock Text="{StaticResource lngValidMetalFinishes}"
                               Style="{StaticResource TitleTextBlockStyle}" />
                </DockPanel>
                <!--#endregion-->
            </Grid>
        </Border>
        <!--#endregion-->

        <!--#region 3. Specific Constraints -->

        <StackPanel Grid.Row="4"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Margin="0,10,0,10">
            <TextBlock Style="{StaticResource CHeaderTextBlock}"
                       Text="{StaticResource lngSpecificConstraints}"
                       TextDecorations="Underline"
                       ToolTipService.Placement="Right"
                       ToolTipService.InitialShowDelay="0"
                       ToolTipService.VerticalOffset="-17.5"
                       ToolTip="{StaticResource lngSpecificConstraintsTooltip}" />
        </StackPanel>

        <StackPanel Grid.Row="5"
                    Grid.Column="0">
            <ContentControl x:Name="SpecificConstraintsContentPresenter"
                            Content="{Binding}"
                            d:DataContext="{d:DesignInstance {x:Type vmc:Constraints94ViewModel} , IsDesignTimeCreatable=True}">
                <ContentControl.Resources>
                    <!--#region 1. 9S DataTemplate -->
                    <DataTemplate x:Name="DataTemplate9S"
                                  DataType="{x:Type vmc:Constraints9SViewModel}">
                        <StackPanel x:Name="Constraints9SStackPanel"
                                    Orientation="Horizontal">
                            <StackPanel>
                                <!--#region A. MaxDoorGlassLength -->
                                <StackPanel x:Name="MaxDoorGlassLengthStackPanel"
                                            Margin="0,5">
                                    <DockPanel>
                                        <cc:Icon ToolTip="{StaticResource lngMaxDoorGlassLengthTooltip}" />
                                        <TextBlock x:Name="MaxDoorGlassLengthTextBlock"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngMaxDoorGlassLength}">
                                        </TextBlock>
                                    </DockPanel>
                                    <TextBox x:Name="MaxDoorGlassLengthTextBox"
                                             d:Text="1000"
                                             Text="{Binding MaxDoorGlassLength ,Mode=TwoWay , TargetNullValue=''}">
                                        <TextBox.ToolTip>
                                            <StackPanel>
                                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                                <TextBlock Text="{Binding Defaults.MaxDoorGlassLength, StringFormat={}{0}mm}" />
                                            </StackPanel>
                                        </TextBox.ToolTip>
                                        <i:Interaction.Behaviors>
                                            <b:TextBoxNumericInputBehavior Precision="0"
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True" />
                                        </i:Interaction.Behaviors>
                                    </TextBox>
                                </StackPanel>
                                <!--#endregion-->

                                <!--#region Β. MinDoorDistanceFromWallOpened -->
                                <StackPanel x:Name="MinDoorDistanceFromWallOpenedStackPanel"
                                            Margin="0,5">
                                    <DockPanel>
                                        <cc:Icon ToolTip="{StaticResource lngMinDoorDistanceFromWallOpenedTooltip}" />
                                        <TextBlock x:Name="MinDoorDistanceFromWallOpenedTextBlock"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngMinDoorDistanceFromWallOpened}">
                                        </TextBlock>
                                    </DockPanel>
                                    <TextBox x:Name="MinDoorDistanceFromWallOpenedTextBox"
                                             d:Text="1000"
                                             Text="{Binding MinDoorDistanceFromWallOpened ,Mode=TwoWay ,TargetNullValue=''}">
                                        <TextBox.ToolTip>
                                            <StackPanel>
                                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                                <TextBlock Text="{Binding Defaults.MinDoorDistanceFromWallOpened, StringFormat={}{0}mm}" />
                                            </StackPanel>
                                        </TextBox.ToolTip>
                                        <i:Interaction.Behaviors>
                                            <b:TextBoxNumericInputBehavior Precision="0"
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True" />
                                        </i:Interaction.Behaviors>
                                    </TextBox>
                                </StackPanel>
                                <!--#endregion-->

                                <!--#region C. StepHeightTollerance -->
                                <StackPanel x:Name="StepHeightTolleranceStackPanel"
                                            Margin="0,5">
                                    <DockPanel>
                                        <cc:Icon ToolTip="{StaticResource lngStepHeightTolleranceTooltip}" />
                                        <TextBlock x:Name="StepHeightTolleranceTextBlock"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngStepHeightTollerance}">
                                        </TextBlock>
                                    </DockPanel>
                                    <TextBox x:Name="StepHeightTolleranceTextBox"
                                             d:Text="1000"
                                             Text="{Binding StepHeightTollerance ,Mode=TwoWay ,TargetNullValue=''}">
                                        <TextBox.ToolTip>
                                            <StackPanel>
                                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                                <TextBlock Text="{Binding Defaults.StepHeightTollerance, StringFormat={}{0}mm}" />
                                            </StackPanel>
                                        </TextBox.ToolTip>
                                        <i:Interaction.Behaviors>
                                            <b:TextBoxNumericInputBehavior Precision="0"
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True" />
                                        </i:Interaction.Behaviors>
                                    </TextBox>
                                </StackPanel>
                                <!--#endregion-->
                            </StackPanel>
                            <hc:Divider Style="{StaticResource VerticalDivider}" />

                            <StackPanel>

                                <!--#region D. Overlap -->
                                <StackPanel x:Name="OverlapStackPanel"
                                            Margin="0,5">
                                    <DockPanel>
                                        <cc:Icon ToolTip="{StaticResource lngOverlapTooltip}" />
                                        <TextBlock x:Name="OverlapTextBlock"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngOverlap}">
                                        </TextBlock>
                                    </DockPanel>
                                    <TextBox x:Name="OverlapTextBox"
                                             d:Text="1000"
                                             Text="{Binding OverlapEPIK ,Mode=TwoWay , TargetNullValue=''}">
                                        <TextBox.ToolTip>
                                            <StackPanel>
                                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                                <TextBlock Text="{Binding Defaults.Overlap, StringFormat={}{0}mm}" />
                                            </StackPanel>
                                        </TextBox.ToolTip>
                                        <i:Interaction.Behaviors>
                                            <b:TextBoxNumericInputBehavior Precision="0"
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True" />
                                        </i:Interaction.Behaviors>
                                    </TextBox>
                                </StackPanel>
                                <!--#endregion-->

                                <!--#region E. CoverDistance -->
                                <StackPanel x:Name="CoverDistanceStackPanel"
                                            Margin="0,5">
                                    <DockPanel>
                                        <cc:Icon ToolTip="{StaticResource lngCoverDistanceTooltip}" />
                                        <TextBlock x:Name="CoverDistanceTextBlock"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngCoverDistance}">
                                        </TextBlock>
                                    </DockPanel>
                                    <TextBox x:Name="CoverDistanceTextBox"
                                             d:Text="1000"
                                             Text="{Binding CoverDistance ,Mode=TwoWay , TargetNullValue=''}">
                                        <TextBox.ToolTip>
                                            <StackPanel>
                                                <TextBlock Text="{StaticResource lngDefaultValue}" />
                                                <TextBlock Text="{Binding Defaults.CoverDistance, StringFormat={}{0}mm}" />
                                            </StackPanel>
                                        </TextBox.ToolTip>
                                        <i:Interaction.Behaviors>
                                            <b:TextBoxNumericInputBehavior Precision="0"
                                                                           InputMode="DigitInput" />
                                        </i:Interaction.Behaviors>
                                    </TextBox>
                                </StackPanel>
                                <!--#endregion-->

                                <!--#region F. SealerLoLengthCorrection -->
                                <StackPanel x:Name="SealerLoLengthCorrectionStackPanel"
                                            Margin="0,5"
                                            HorizontalAlignment="Center">
                                    <DockPanel>
                                        <cc:Icon ToolTip="{StaticResource lngSealerL0LengthCorrectionTooltip}" />
                                        <TextBlock x:Name="SealerLoLengthCorrectionTextBlock"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngSealerL0LengthCorrection}">
                                        </TextBlock>
                                    </DockPanel>
                                    <TextBox x:Name="SealerLoLengthCorrectionTextBox"
                                             d:Text="1000"
                                             Text="{Binding SealerL0LengthCorrection ,Mode=TwoWay , TargetNullValue=''}">
                                        <TextBox.ToolTip>
                                            <StackPanel>
                                                <TextBlock Text="{StaticResource lngDefaultValue}" />
