<UserControl x:Class="BronzeFactoryApplication.Views.ComponentsUC.EditModelUserControl.LiveEditPartUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:local="clr-namespace:BronzeFactoryApplication.Views.ComponentsUC.EditModelUserControl"
             xmlns:cachedImage="clr-namespace:CachedImage.Core;assembly=CachedImage.Core"
             xmlns:cc="clr-namespace:TasCustomControlsLibrary.CustomControls;assembly=TasCustomControlsLibrary"
             xmlns:vm="clr-namespace:BronzeFactoryApplication.ViewModels.CabinsViewModels.PartsViewModels.Parts"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:b="clr-namespace:BronzeFactoryApplication.Helpers.Behaviours"
             xmlns:ext="clr-namespace:BronzeFactoryApplication.Helpers.MarkupExtensions"
             xmlns:enums="clr-namespace:ShowerEnclosuresModelsLibrary.Enums;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:partTypeEnum="clr-namespace:ShowerEnclosuresModelsLibrary.Models.CabinAllParts.PartsModels;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:handleTypeEnum="clr-namespace:ShowerEnclosuresModelsLibrary.Models.CabinAllParts.PartsModels.HandlesModels;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:hingeTypeEnum="clr-namespace:ShowerEnclosuresModelsLibrary.Models.CabinAllParts.PartsModels.HingesModels;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:profileTypeEnum="clr-namespace:ShowerEnclosuresModelsLibrary.Models.CabinAllParts.PartsModels.ProfilesModels;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:stripTypeEnum="clr-namespace:ShowerEnclosuresModelsLibrary.Models.CabinAllParts.PartsModels.StripsModels;assembly=ShowerEnclosuresModelsLibrary"
             xmlns:placementEnum="clr-namespace:ShowerEnclosuresModelsLibrary.Models.CabinAllParts.PartsModels.SupportsModels;assembly=ShowerEnclosuresModelsLibrary"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=vm:LiveEditPartViewModel}">
    <UserControl.Resources>
        
        <!--#region 1.Styles -->
        <Style TargetType="{x:Type cc:Icon}" BasedOn="{StaticResource CInfoTooltipIcon}"/>
        <Style TargetType="{x:Type TextBox}" BasedOn="{StaticResource TextBoxExtend}">
            <Setter Property="ToolTipService.Placement" Value="Right"/>
            <Setter Property="ToolTipService.VerticalOffset" Value="-17.5"/>
            <Setter Property="ToolTipService.InitialShowDelay" Value="0"/>
            <Setter Property="ToolTipService.BetweenShowDelay" Value="0"/>
            <Setter Property="ToolTipService.ShowsToolTipOnKeyboardFocus" Value="True"/>
            <Setter Property="Width" Value="250"/>
        </Style>
        <Style x:Key="VerticalDivider" TargetType="{x:Type hc:Divider}" BasedOn="{StaticResource DividerBaseStyle}">
            <Setter Property="Orientation" Value="Vertical"/>
            <Setter Property="Width" Value="25"/>
            <Setter Property="Height" Value="120"/>
            <Setter Property="LineStroke" Value="{DynamicResource PrimaryBrush}"/>
            <Setter Property="LineStrokeThickness" Value="0.5"/>
        </Style>
        <Style TargetType="{x:Type ComboBox}" BasedOn="{StaticResource ComboBoxExtend}">
            <Setter Property="Width" Value="250"/>
        </Style>
        <!--#endregion-->

        <!--#region 2.DataTemplates -->

        <!--#region A. Angle DataTemplate -->

        <DataTemplate x:Key="CabinAngleDataTemplate" DataType="{x:Type vm:LiveEditAngleViewModel}">
            <StackPanel x:Name="CabinAngleStackPanel" 
                                Orientation="Vertical" 
                                HorizontalAlignment="Center">
                <StackPanel Orientation="Horizontal">
                    <StackPanel>

                        <!--#region A. AngleDistanceFromDoor -->
                        <StackPanel 
                                    x:Name="AngleDistanceFromDoorStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngAngleDistanceFromDoorTooltip}"/>
                                <TextBlock 
                                            x:Name="AngleDistanceFromDoorTextBlock"
                                            Style="{StaticResource TitleTextBlockStyle}"
                                            Text="{StaticResource lngAngleDistanceFromDoor}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                        x:Name="AngleDistanceFromDoorTextBox" d:Text="1000"
                                        Text="{Binding AngleDistanceFromDoor ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                       InputMode="DigitInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                    <hc:Divider 
                                Style="{StaticResource VerticalDivider}"
                                Height="50"/>

                    <StackPanel>

                        <!--#region D. AngleLengthL0 -->
                        <StackPanel 
                                    x:Name="AngleLengthL0StackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngAngleLengthL0Tooltip}"/>
                                <TextBlock 
                                            x:Name="AngleLengthL0TextBlock"
                                            Style="{StaticResource TitleTextBlockStyle}"
                                            Text="{StaticResource lngAngleLengthL0}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                        x:Name="AngleLengthL0TextBox" d:Text="1000"
                                        Text="{Binding AngleLengthL0 ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                       InputMode="DigitInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                </StackPanel>

                <!--#region D. AngleHeight -->
                <StackPanel 
                            x:Name="AngleHeightStackPanel"
                            Margin="0,5" HorizontalAlignment="Center">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngAngleHeightTooltip}"/>
                        <TextBlock 
                                    x:Name="AngleHeightTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngAngleHeight}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox 
                                x:Name="AngleHeightTextBox" d:Text="1000"
                                Text="{Binding AngleHeight ,Mode=TwoWay , TargetNullValue=''}">
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior 
                                        Precision="0" 
                                        InputMode="DigitInput"
                                        JustPositivDecimalInput="True"/>
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->
            </StackPanel>
        </DataTemplate>

        <!--#endregion-->

        <!--#region B. Handle DataTemplate -->

        <DataTemplate x:Key="CabinHandleDataTemplate" DataType="{x:Type vm:LiveEditHandleViewModel}">
            <StackPanel x:Name="CabinHandleStackPanel" 
                                Orientation="Vertical" 
                                HorizontalAlignment="Center">
                <StackPanel Orientation="Horizontal">
                    <StackPanel>

                        <!--#region A. HandleType -->
                        <StackPanel x:Name="HandleTypeStackPanel"
                                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHandleTypeTooltip}"/>
                                <TextBlock 
                                            x:Name="HandleTypeTextBlock" DockPanel.Dock="Left"
                                            Style="{StaticResource TitleTextBlockStyle}"
                                            Text="{StaticResource lngHandleType}">
                                </TextBlock>
                            </DockPanel>
                            <ComboBox x:Name="HandleTypeComboBox"
                                              d:ItemsSource="{x:Null}"
                                              ItemsSource="{Binding Source={ext:BindingSourceEnumValues {x:Type handleTypeEnum:CabinHandleType}}, Mode=OneWay}"
                                              SelectedItem="{Binding  HandleType, Mode=OneWay}"
                                              IsEnabled="False">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter} , TargetNullValue=''}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region Β. HandleLengthToGlass -->
                        <StackPanel x:Name="HandleLengthToGlassStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHandleLengthToGlassTooltip}"/>
                                <TextBlock x:Name="HandleLengthToGlassTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngHandleLengthToGlass}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="HandleLengthToGlassTextBox" d:Text="1000"
                                     Text="{Binding HandleLengthToGlass ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                       InputMode="DigitInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region C. HandleHeightToGlass -->
                        <StackPanel x:Name="HandleHeightToGlassStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHandleHeightToGlassTooltip}"/>
                                <TextBlock x:Name="HandleHeightToGlassTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngHandleHeightToGlass}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="HandleHeightToGlassTextBox" d:Text="1000"
                                     Text="{Binding HandleHeightToGlass ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                       InputMode="DigitInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region D. HandleEdgesCornerRadius -->
                        <StackPanel x:Name="HandleEdgesCornerRadiusStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHandleEdgesCornerRadiusTooltip}"/>
                                <TextBlock x:Name="HandleEdgesCornerRadiusTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngHandleEdgesCornerRadius}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="HandleEdgesCornerRadiusTextBox" d:Text="1000"
                                     Text="{Binding HandleEdgesCornerRadius ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="1" 
                                                                       InputMode="DecimalInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                    <hc:Divider Style="{StaticResource VerticalDivider}"/>

                    <StackPanel>

                        <!--#region E. HandleOuterThickness -->
                        <StackPanel x:Name="HandleOuterThicknessStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHandleOuterThicknessTooltip}"/>
                                <TextBlock x:Name="HandleOuterThicknessTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngHandleOuterThickness}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="HandleOuterThicknessTextBox" d:Text="1000"
                                     Text="{Binding HandleOuterThickness ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="1" 
                                                                       InputMode="DecimalInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region F. HandleComfortDistance -->
                        <StackPanel x:Name="HandleComfortDistanceStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHandleComfortDistanceTooltip}"/>
                                <TextBlock x:Name="HandleComfortDistanceTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngHandleComfortDistance}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="HandleComfortDistanceTextBox" d:Text="1000"
                                     Text="{Binding HandleComfortDistance ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                       InputMode="DigitInput"
                                                                       JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region G. MinimumDistanceFromEdge -->
                        <StackPanel x:Name="MinimumDistanceFromEdgeStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngMinimumDistanceFromEdgeTooltip}"/>
                                <TextBlock x:Name="MinimumDistanceFromEdgeTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngMinimumDistanceFromEdge}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="MinimumDistanceFromEdgeTextBox" d:Text="1000"
                                     Text="{Binding MinimumDistanceFromEdge ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                   InputMode="DigitInput"
                                                                   JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region H.Is Circular -->

                        <TextBlock Text="Σχήμα Λαβής" 
                                           Style="{StaticResource CSmallInfoTextBlock}"
                                           Margin="0,10,0,5" HorizontalAlignment="Center"/>
                        <TextBlock 
                                           Margin="0" HorizontalAlignment="Center" Tag="{Binding IsCircularHandle}">
                            <TextBlock.Style>
                                <Style TargetType="TextBlock" BasedOn="{StaticResource TitleTextBlockStyle}">
                                    <Setter Property="Text" Value="{StaticResource lngStraightHandle}"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Tag}" Value="True">
                                            <Setter Property="Text" Value="{StaticResource lngCircularHandle}"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </TextBlock.Style>
                        </TextBlock>

                        <!--#endregion-->

                    </StackPanel>
                </StackPanel>

                <!--EMPTY-->

            </StackPanel>
        </DataTemplate>

        <!--#endregion-->

        <!--#region C. Hinge DataTemplate -->

        <!--#region 1.Hinge Generic -->
        <DataTemplate x:Key="CabinHingeDataTemplate" DataType="{x:Type vm:LiveEditHingeViewModel}">
                <StackPanel Orientation="Horizontal"
                            HorizontalAlignment="Center">
                    <StackPanel>

                        <!--#region A. HingeType -->
                        <StackPanel x:Name="HingeTypeStackPanel"
                                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHingeTypeTooltip}"/>
                                <TextBlock x:Name="HingeTypeTextBlock" DockPanel.Dock="Left"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngHingeType}">
                                </TextBlock>
                            </DockPanel>
                            <ComboBox x:Name="HingeTypeComboBox"
                                              d:ItemsSource="{x:Null}"
                                              ItemsSource="{Binding Source={ext:BindingSourceEnumValues {x:Type hingeTypeEnum:CabinHingeType}},Mode=OneWay}"
                                              SelectedItem="{Binding  HingeType, Mode=OneWay}"
                                              IsEnabled="False">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter} , TargetNullValue=''}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region Β. LengthViewHinge -->
                        <StackPanel x:Name="LengthViewHingeStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngLengthViewHingeTooltip}"/>
                                <TextBlock x:Name="LengthViewHingeTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngLengthViewHinge}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="LengthViewHingeTextBox" d:Text="1000"
                                     Text="{Binding LengthView ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                    <hc:Divider Style="{StaticResource VerticalDivider}" Height="100"/>

                    <StackPanel>

                        <!--#region C. HeightViewHinge -->
                        <StackPanel x:Name="HeightViewHingeStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHeightViewHingeTooltip}"/>
                                <TextBlock x:Name="HeightViewHingeTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngHeightViewHinge}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="HeightViewHingeTextBox" d:Text="1000"
                                     Text="{Binding HeightView ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region D. GlassGapAER -->
                        <StackPanel x:Name="GlassGapAERStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngGlassGapAERHingeTooltip}"/>
                                <TextBlock x:Name="GlassGapAERTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngGlassGapAERHinge}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="GlassGapAERTextBox" d:Text="1000"
                                     Text="{Binding GlassGapAER ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                </StackPanel>
        </DataTemplate>
        <!--#endregion-->

        <!--#region 2.Glass To Glass Hinge -->
        <DataTemplate x:Key="GlassToGlassHingeDataTemplate" DataType="{x:Type vm:LiveEditGlassToGlassHingeViewModel}">
            <StackPanel 
                Orientation="Vertical"
                HorizontalAlignment="Center">
                <ContentPresenter ContentTemplate="{StaticResource CabinHingeDataTemplate}"/>

                <!--#region 1. InDoorLengthHinge -->
                <StackPanel 
                        x:Name="InDoorLengthHingeStackPanel"
                        Margin="0,5"
                    HorizontalAlignment="Center">
                    <DockPanel>
                        <cc:Icon ToolTip="{StaticResource lngInDoorLengthHingeTooltip}"/>
                        <TextBlock 
                                x:Name="InDoorLengthHingeTextBlock"
                                Style="{StaticResource TitleTextBlockStyle}"
                                Text="{StaticResource lngInDoorLengthHinge}">
                        </TextBlock>
                    </DockPanel>
                    <TextBox 
                            x:Name="InDoorLengthHingeTextBox" d:Text="1000"
                            Text="{Binding InDoorLength ,Mode=TwoWay , TargetNullValue=''}">
                        <i:Interaction.Behaviors>
                            <b:TextBoxNumericInputBehavior 
                                    Precision="0" 
                                    InputMode="DigitInput"
                                    JustPositivDecimalInput="True"/>
                        </i:Interaction.Behaviors>
                    </TextBox>
                </StackPanel>
                <!--#endregion-->

            </StackPanel>
        </DataTemplate>
        <!--#endregion-->

        <!--#region 3.Hinge 9B -->
        <DataTemplate x:Key="Hinge9BDataTemplate" DataType="{x:Type vm:LiveEditHinge9BHingeViewModel}">
            <StackPanel 
                Orientation="Vertical"
                HorizontalAlignment="Center">
                <ContentPresenter ContentTemplate="{StaticResource CabinHingeDataTemplate}"/>
                <StackPanel Orientation="Horizontal">
                    <StackPanel>
                        <!--#region 1. HingeOverlappingHeight -->
                        <StackPanel 
                            x:Name="HingeOverlappingHeightStackPanel"
                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngHingeOverlappingHeightTooltip}"/>
                                <TextBlock 
                                    x:Name="HingeOverlappingHeightTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngHingeOverlappingHeight}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                x:Name="HingeOverlappingHeightTextBox" 
                                d:Text="1000"
                                Text="{Binding HingeOverlappingHeight ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior 
                                        Precision="0" 
                                        InputMode="DigitInput"
                                        JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region 2. SupportTubeLength -->
                        <StackPanel 
                            x:Name="SupportTubeLengthStackPanel"
                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngSupportTubeLengthTooltip}"/>
                                <TextBlock 
                                    x:Name="SupportTubeLengthTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngSupportTubeLength}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                x:Name="SupportTubeLengthTextBox" 
                                d:Text="1000"
                                Text="{Binding SupportTubeLength ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior 
                                        Precision="0" 
                                        InputMode="DigitInput"
                                        JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->
                    </StackPanel>
                    <hc:Divider Style="{StaticResource VerticalDivider}" Height="100"/>
                    <StackPanel>
                        <!--#region 3. SupportTubeHeight -->
                        <StackPanel 
                            x:Name="SupportTubeHeightStackPanel"
                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngSupportTubeHeightTooltip}"/>
                                <TextBlock 
                                    x:Name="SupportTubeHeightTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngSupportTubeHeight}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                x:Name="SupportTubeHeightTextBox" 
                                d:Text="1000"
                                Text="{Binding SupportTubeHeight ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior 
                                        Precision="0" 
                                        InputMode="DigitInput"
                                        JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region 4. CornerRadiusInGlass -->
                        <StackPanel 
                            x:Name="CornerRadiusInGlassStackPanel"
                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngCornerRadiusInGlassTooltip}"/>
                                <TextBlock 
                                    x:Name="CornerRadiusInGlassTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngCornerRadiusInGlass}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                x:Name="CornerRadiusInGlassTextBox" d:Text="1000"
                                Text="{Binding CornerRadiusInGlass ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior 
                                        Precision="0" 
                                        InputMode="DigitInput"
                                        JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->
                    </StackPanel>
                </StackPanel>
            </StackPanel>
        </DataTemplate>
        <!--#endregion-->

        <!--#region 4.Hinge DB -->
        <DataTemplate x:Key="HingeDBDataTemplate" DataType="{x:Type vm:LiveEditHingeDBViewModel}">
            <StackPanel 
                Orientation="Vertical"
                HorizontalAlignment="Center">
                <ContentPresenter ContentTemplate="{StaticResource CabinHingeDataTemplate}"/>
                <StackPanel Orientation="Horizontal">
                    <StackPanel>
                        <!--#region 1. InnerHeight -->
                        <StackPanel 
                            x:Name="InnerHeightStackPanel"
                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngInnerHeightHingeTooltip}"/>
                                <TextBlock 
                                    x:Name="InnerHeightTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngInnerHeightHinge}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox 
                                x:Name="InnerHeightTextBox" 
                                d:Text="1000"
                                Text="{Binding InnerHeight ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior 
                                        Precision="0" 
                                        InputMode="DigitInput"
                                        JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->
                    </StackPanel>
                    <hc:Divider Style="{StaticResource VerticalDivider}" Height="40"/>
                    <StackPanel>
                        <!--#region 2. WallPlateThicknessView -->
                        <StackPanel 
                            x:Name="WallPlateThicknessViewStackPanel"
                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngWallPlateThicknessViewTooltip}"/>
                                <TextBlock 
                                    x:Name="WallPlateThicknessViewTextBlock"
                                    Style="{StaticResource TitleTextBlockStyle}"
                                    Text="{StaticResource lngWallPlateThicknessView}">
                                </TextBlock>
