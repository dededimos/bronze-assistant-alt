            </StackPanel>
        </DataTemplate>
        
        <!--#endregion-->

        <!--#endregion-->

        <!--#region G. SupportBar DataTemplate -->

        <DataTemplate x:Key="SupportBarDataTemplate" DataType="{x:Type vm:LiveEditSupportBarViewModel}">
            <StackPanel x:Name="SupportBarStackPanel" 
                                Orientation="Vertical" 
                                HorizontalAlignment="Center">
                <StackPanel Orientation="Horizontal">
                    <StackPanel>

                        <!--#region A. Placement -->
                        <StackPanel x:Name="PlacementStackPanel"
                                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngPlacementSupportBarTooltip}"/>
                                <TextBlock x:Name="PlacementTextBlock" DockPanel.Dock="Left"
                                                   Style="{StaticResource TitleTextBlockStyle}"
                                                   Text="{StaticResource lngPlacementSupportBar}">
                                </TextBlock>
                            </DockPanel>
                            <ComboBox x:Name="PlacementComboBox"
                                              d:ItemsSource="{x:Null}"
                                              ItemsSource="{Binding Source={ext:BindingSourceEnumValues {x:Type placementEnum:SupportBarPlacement}}, Mode=OneWay}"
                                              SelectedItem="{Binding  Placement, Mode=TwoWay}">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Converter={StaticResource languageConverter} , TargetNullValue=''}"/>
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region Β. ClampViewLength -->
                        <StackPanel x:Name="ClampViewLengthStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngClampLengthViewSupportBarTooltip}"/>
                                <TextBlock x:Name="ClampViewLengthTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngClampLengthViewSupportBar}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="ClampViewLengthTextBox" d:Text="1000"
                                     Text="{Binding ClampViewLength ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region C. ClampViewHeight -->
                        <StackPanel x:Name="ClampViewHeightStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngClampHeightViewSupportBarTooltip}"/>
                                <TextBlock x:Name="ClampViewHeightTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngClampHeightViewSupportBar}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="ClampViewHeightTextBox" d:Text="1000"
                                     Text="{Binding ClampViewHeight ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                    <hc:Divider Style="{StaticResource VerticalDivider}"/>

                    <StackPanel>

                        <!--#region D. ClampCenterDistanceFromGlassDefault -->
                        <StackPanel x:Name="ClampFromGlassDefaultStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngClampCenterDistanceFromGlassDefaultTooltip}"/>
                                <TextBlock x:Name="ClampFromGlassDefaultTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngClampCenterDistanceFromGlassDefault}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="ClampFromGlassDefaultTextBox" d:Text="1000"
                                     Text="{Binding ClampCenterDistanceFromGlassDefault ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior 
                                                Precision="0" 
                                                InputMode="DigitInput"
                                                JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region E. ClampCenterDistanceFromGlass -->
                        <StackPanel x:Name="ClampFromGlassStackPanel"
                                            IsEnabled="False"
                                            Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngClampCenterDistanceFromGlassTooltip}"/>
                                <TextBlock x:Name="ClampFromGlassTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngClampCenterDistanceFromGlass}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="ClampFromGlassTextBox" d:Text="1000"
                                     Text="{Binding ClampCenterDistanceFromGlass ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                           InputMode="DigitInput"
                                                                           JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                        <!--#region F. OutOfGlassHeightSupportBar -->
                        <StackPanel x:Name="OutOfGlassHeightSupportBarStackPanel"
                                    Margin="0,5">
                            <DockPanel>
                                <cc:Icon ToolTip="{StaticResource lngOutOfGlassHeightSupportBarTooltip}"/>
                                <TextBlock x:Name="OutOfGlassHeightSupportBarTextBlock"
                                           Style="{StaticResource TitleTextBlockStyle}"
                                           Text="{StaticResource lngOutOfGlassHeightSupportBar}">
                                </TextBlock>
                            </DockPanel>
                            <TextBox x:Name="OutOfGlassHeightSupportBarTextBox" d:Text="1000"
                                     Text="{Binding OutOfGlassHeight ,Mode=TwoWay , TargetNullValue=''}">
                                <i:Interaction.Behaviors>
                                    <b:TextBoxNumericInputBehavior Precision="0" 
                                                                   InputMode="DigitInput"
                                                                   JustPositivDecimalInput="True"/>
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </StackPanel>
                        <!--#endregion-->

                    </StackPanel>
                </StackPanel>

                <!--EMPTY-->

            </StackPanel>
        </DataTemplate>

        <!--#endregion-->

        <!--#endregion-->

    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="20"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>

        <!--#region 1.Common Part Properties -->
        <StackPanel Grid.Row="0" 
                    Grid.Column="0">
            <TextBlock Text="{StaticResource lngCommonPartProperties}"
                       Style="{StaticResource CHeaderTextBlock}"
                       TextDecorations="Underline"
                       Margin="0,0,0,10"/>
        </StackPanel>

        <StackPanel x:Name="CommonConstraintsStackPanel"
                    HorizontalAlignment="Center"
                    Grid.Row="1" 
                    Grid.Column="0">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <StackPanel>

                    <!--#region A. Code -->
                    <StackPanel 
                        x:Name="CodeStackPanel"
                        Margin="0,5">
                        <DockPanel>
                            <cc:Icon ToolTip="{StaticResource lngPartCodeTooltip}"/>
                            <TextBlock 
                                x:Name="CodeTextBlock"
                                Style="{StaticResource TitleTextBlockStyle}"
                                Text="{StaticResource lngCode}"/>
                        </DockPanel>
                        <TextBox 
                            x:Name="CodeTextBox" 
                            d:Text="CODE" MaxLength="40"
                            Text="{Binding Code ,Mode=TwoWay, TargetNullValue=''}">
                        </TextBox>
                    </StackPanel>
                    <!--#endregion-->

                    <!--#region B. Description Default -->
                    <StackPanel 
                        x:Name="DescriptionStackPanel"
                        Margin="0,5">
                        <DockPanel>
                            <cc:Icon ToolTip="{StaticResource lngPartDescriptionTooltip}"/>
                            <TextBlock 
                                x:Name="DescriptionTextBlock"
                                Style="{StaticResource TitleTextBlockStyle}"
                                Text="{StaticResource lngPartDescription}">
                            </TextBlock>
                        </DockPanel>
                        <TextBox 
                            x:Name="DescriptionTextBox" 
                            d:Text="DESCRIPTION"
                            Text="{Binding Description ,Mode=TwoWay, TargetNullValue=''}">
                        </TextBox>
                    </StackPanel>
                    <!--#endregion-->

                </StackPanel>

                <hc:Divider Style="{StaticResource VerticalDivider}"/>

                <StackPanel>

                    <!--#region C. Material -->
                    <StackPanel 
                        x:Name="MaterialStackPanel"
                        Margin="0,5">
                        <DockPanel>
                            <cc:Icon ToolTip="{StaticResource lngPartMaterialTooltip}"/>
                            <TextBlock 
                                x:Name="MaterialTextBlock" 
                                DockPanel.Dock="Left"
                                Style="{StaticResource TitleTextBlockStyle}"
                                Text="{StaticResource lngMaterial}">
                            </TextBlock>
                        </DockPanel>
                        <ComboBox 
                            x:Name="MaterialComboBox"
                            ItemsSource="{Binding Source={ext:BindingSourceEnumValues {x:Type enums:MaterialType}}}"
                            SelectedItem="{Binding Material , Mode=TwoWay}">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource languageConverter} , TargetNullValue=''}"/>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                    </StackPanel>
                    <!--#endregion-->

                    <!--#region D. Part Type -->
                    <StackPanel x:Name="TypeStackPanel" Margin="0,5">
                        <DockPanel>
                            <cc:Icon ToolTip="{StaticResource lngPartTypeTooltip}"/>
                            <TextBlock 
                                x:Name="TypeTextBlock" 
                                DockPanel.Dock="Left"
                                Style="{StaticResource TitleTextBlockStyle}"
                                Text="{StaticResource lngType}">
                            </TextBlock>
                        </DockPanel>
                        <ComboBox 
                            x:Name="TypeComboBox"
                            ItemsSource="{Binding Source={ext:BindingSourceEnumValues {x:Type partTypeEnum:CabinPartType}}}"
                            SelectedItem="{Binding PartType , Mode=OneWay}"
                            IsEnabled="False">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding Converter={StaticResource languageConverter} , TargetNullValue=''}"/>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                    </StackPanel>
                    <!--#endregion-->

                </StackPanel>
            </StackPanel>

            <!--#region E. PhotoPath -->
            <StackPanel x:Name="PhotoPathStackPanel"
                        Margin="5,5,0,10">
                <DockPanel>
                    <cc:Icon>
                        <cc:Icon.ToolTip>
                            <StackPanel>
                                <TextBlock Text="{StaticResource lngPartPhotoPathTooltip}"/>
                                <TextBlock Text="{StaticResource lngCurrentPlacePhotoLink}" Margin="0,10,0,5" HorizontalAlignment="Center"/>
                                <cachedImage:Image ImageUrl="{Binding PhotoPath}"
                                                   FailedImage="{StaticResource DefaultNoImage}"
                                                   MaxWidth="100" MaxHeight="100" Stretch="Uniform"/>
                            </StackPanel>
                        </cc:Icon.ToolTip>
                    </cc:Icon>
                    <TextBlock x:Name="PhotoPathTextBlock"
                           Style="{StaticResource TitleTextBlockStyle}"
                           Text="{StaticResource lngPhotoPath}">
                    </TextBlock>
                </DockPanel>
                <TextBox x:Name="PhotoPathTextBox" d:Text="PhotoPath" Width="535"
                         Text="{Binding PhotoPath ,Mode=TwoWay, TargetNullValue=''}">
                </TextBox>
            </StackPanel>
            <!--#endregion-->

        </StackPanel>



        <!--#endregion-->
        
        <!--#region 2.Specific Properties -->
        <StackPanel Grid.Row="2" Margin="0,15,0,0"
                    Grid.Column="0">
            <TextBlock Text="{Binding PartType, Converter={StaticResource languageConverter}}"
                       Style="{StaticResource CHeaderTextBlock}"
                       TextDecorations="Underline"/>
        </StackPanel>

        <ContentControl Content="{Binding .}" 
                        MinWidth="300" 
                        Margin="0,20,0,0"
                        Grid.Row="3" 
                        Grid.Column="0"
                        d:DataContext="{d:DesignInstance Type={x:Type vm:LiveEditSupportViewModel},IsDesignTimeCreatable=True}">

            <ContentControl.Resources>

                <!--#region A. Angle DataTemplate -->

                <DataTemplate x:Name="CabinAngleDataTemplate" DataType="{x:Type vm:LiveEditAngleViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource CabinAngleDataTemplate}"/>
                </DataTemplate>

                <!--#endregion-->

                <!--#region B. Handle DataTemplate -->

                <DataTemplate x:Name="CabinHandleDataTemplate" DataType="{x:Type vm:LiveEditHandleViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource CabinHandleDataTemplate}"/>
                </DataTemplate>

                <!--#endregion-->

                <!--#region C. Hinge DataTemplate -->
                <DataTemplate DataType="{x:Type vm:LiveEditGlassToGlassHingeViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource GlassToGlassHingeDataTemplate}"/>
                </DataTemplate>
                <DataTemplate DataType="{x:Type vm:LiveEditHinge9BHingeViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource Hinge9BDataTemplate}"/>
                </DataTemplate>
                <DataTemplate DataType="{x:Type vm:LiveEditHingeDBViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource HingeDBDataTemplate}"/>
                </DataTemplate>
                <DataTemplate x:Name="CabinHingeDataTemplate" DataType="{x:Type vm:LiveEditHingeViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource CabinHingeDataTemplate}"/>
                </DataTemplate>

                <!--#endregion-->

                <!--#region D. Profile DataTemplate -->
                
                <!--#region ProfileHinge DataTemplate -->
                <DataTemplate x:Key="ProfileHingeDataTemplate" DataType="{x:Type vm:LiveEditProfileHingeViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource ProfileHingeDataTemplate}"/>
                </DataTemplate>
                <!--#endregion-->

                <!--#region Profile DataTemplate -->
                <DataTemplate DataType="{x:Type vm:LiveEditProfileViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource ProfileDataTemplate}"/>
                </DataTemplate>
                <!--#endregion-->

                <!--#endregion-->

                <!--#region E. GlassStrip DataTemplate -->

                <DataTemplate x:Name="GlassStripDataTemplate" DataType="{x:Type vm:LiveEditStripViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource GlassStripDataTemplate}"/>
                </DataTemplate>

                <!--#endregion-->

                <!--#region F. Support DataTemplate -->

                <DataTemplate x:Name="FloorStopperDataTemplate" DataType="{x:Type vm:LiveEditFloorStopperViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource FloorStopperDataTemplate}"/>
                </DataTemplate>
                
                <DataTemplate x:Name="SupportDataTemplate" DataType="{x:Type vm:LiveEditSupportViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource SupportDataTemplate}"/>
                </DataTemplate>

                <!--#endregion-->

                <!--#region G. SupportBar DataTemplate -->

                <DataTemplate x:Name="SupportBarDataTemplate" DataType="{x:Type vm:LiveEditSupportBarViewModel}">
                    <ContentPresenter ContentTemplate="{StaticResource SupportBarDataTemplate}"/>
                </DataTemplate>

                <!--#endregion-->

                <!--#region 0. Empty -->
                <DataTemplate x:Name="EmptyEditPartDataTemplate" DataType="{x:Type vm:LiveEditPartViewModel}">

                </DataTemplate>
                <!--#endregion-->

            </ContentControl.Resources>
        </ContentControl>

        <!--#endregion-->
    </Grid>
</UserControl>
